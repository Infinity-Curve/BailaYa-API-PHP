{
    "name": "bailaya/core",
    "version": "1.0.0",
    "description": "A PHP client for accessing the BailaYa public API",
    "type": "library",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "php-http/mock-client": "^1.6"
    },
    "license": "ISC",
    "autoload": {
        "psr-4": {
            "BailaYa\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Infinity Curve"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
